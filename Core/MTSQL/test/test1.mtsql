SET @a = @b;
SET @param2 = 2 * @a;
SET @b = 123;
SELECT intcol, deccol, dblcol, strcol, dtcol INTO @intcol, @deccol, @dblcol, @strcol, @dtcol FROM testmtsql WHERE intcol = @param3;
