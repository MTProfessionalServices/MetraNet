          <Category>CountryName</Category>
          <Namespace>Global</Namespace>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <DefaultValue i:nil="true" />
        <Description>Delivery country for the service transaction</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>DeliveryCountry</Name>
