<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            Represents a class for Bank Account payment instrument which is used to populate Account's Entity collection of payment instruments.
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>BankAccountMasked</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>BankAccountMasked</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Bank Name
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>BankName</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Account Holder's Name
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>AccountHoldersName</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The Routing Number for the bank
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>RoutingNumber</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The type of account
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>BankAccountType</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Name>BankAccountType</Name>
          <PropertyBagMode>PropertyBag</PropertyBagMode>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Last four digits of bank account number
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>BankAccountNumberLast4Digits</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>InstrumentId</Name>
        <Required>true</Required>
        <Type>
          <BaseType>UniqueIdentifier</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>