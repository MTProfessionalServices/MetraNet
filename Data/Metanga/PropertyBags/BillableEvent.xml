<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            Simple Usage Type, supporting SKU/Price per Unit style rating
            Usage events should be submitted in collections of up to 1000 events
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>BillableEvent</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>BillableEvent</Name>
    <PropertyBagMode>ExtensibleEntity</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Timestamp of the Usage
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>StartTime</Name>
        <Required>false</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Time at which transaction ended. Requires a StartTime to be passed.
            If Quantity is not set, MetraNet will set the Quantity to the total minutes between
            the start and the end time.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>EndTime</Name>
        <Required>false</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The units used to measure the quantity. The acceptable values can be seen in the
            Units of Measure Management screen in Metanga. Values provided out of the box are
            based on the Unified Code for Units of Measure standard: http://en.wikipedia.org/wiki/Unified_Code_for_Units_of_Measure</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>UnitOfMeasure</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Number of Items
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Quantity</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Decimal</BaseType>
          <ListType>None</ListType>
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureQualifier i:nil="true" />
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The account that originated this charge.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Originator</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Name>Account</Name>
          <PropertyBagMode>ExtensibleEntity</PropertyBagMode>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The product for which Metanga will generate this charge.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Product</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Name>Product</Name>
          <PropertyBagMode>ExtensibleEntity</PropertyBagMode>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>EntityId</Name>
        <Required>false</Required>
        <Type>
          <BaseType>Guid</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>ExternalId</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Name</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>KeyList</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Description</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>KeyList</ListType>
          <Length>255</Length>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>