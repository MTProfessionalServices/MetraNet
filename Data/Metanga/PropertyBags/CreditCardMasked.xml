<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            Represents credit card information which is used to populate Account's Entity collection of payment instruments.
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>CreditCardMasked</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>CreditCardMasked</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the first name of the owner of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>FirstName</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the middle name of the owner of the credit card.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>MiddleName</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the last name of the owner of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>LastName</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the first address line of the owner of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Address1</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the second address line of the owner of the credit card.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Address2</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the third address line of the owner of the credit card.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Address3</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the city of the owner of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>City</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the country code of the owner of the credit card. The code must correspond to ISO 3166-1 alpha-2 standart of Country abbreviation.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>CountryCode</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
             Gets or sets the state of the owner of the credit card. For US this property is required and must consist of 2 characters. It should be a valid US state abbreviation.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>State</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the postal code of the owner of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Postal</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the phone number of the owner of the credit card.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>PhoneNumber</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the e-mail address of the owner of the credit card.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Email</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Gets or sets the type of the credit card. This property is required.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>CreditCardType</Name>
        <Required>true</Required>
        <Type i:type="EnumerationType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Category>CreditCardType</Category>
          <Namespace>metanga</Namespace>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
             Gets or sets the expiration date of the credit card. The format of this property is "mm/yyyy". This property is required.
             </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>ExpirationDate</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Last four digits of bank account number
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>CreditCardNumberLast4Digits</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Card Verification Number for validating before create new or update existing credit card
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>CardSecurityCode</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
             If true skips verification of the Credit Card when creating or updating.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>IsVerificationSkipped</Name>
        <Required>true</Required>
        <Type>
          <BaseType>Boolean</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>InstrumentId</Name>
        <Required>true</Required>
        <Type>
          <BaseType>Guid</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>