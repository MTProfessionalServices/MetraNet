<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            A price interval contains the price that should be
            applied starting on a certain date.
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>PriceScheduleInterval</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>PriceScheduleInterval</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Start date for the price interval
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>StartDate</Name>
        <Required>true</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            A collection of prices that can be applied for different units of measure.
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>UnitPrices</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>List</ListType>
          <Name>UnitPrice</Name>
          <PropertyBagMode>PropertyBag</PropertyBagMode>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>