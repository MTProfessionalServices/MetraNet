<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <Availability>Unknown</Availability>
  <DefaultValue i:nil="true" />
  <Description>
            Represents a class for configuring notifications that are sent using SOAP.
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>SoapNotificationConfiguration</Name>
  <Required>true</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>SoapNotificationConfiguration</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>
            Represents a version of the SOAP standard that is used by your service.
            Metanga supports v1.1 and v1.2.
            </Description>
        <Direction>Input</Direction>
        <IsCore>true</IsCore>
        <Name>ServiceVersion</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>
            Custom headers which users can add
            </Description>
        <Direction>Input</Direction>
        <IsCore>true</IsCore>
        <Name>CustomHeaders</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>KeyList</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description i:nil="true" />
        <Direction>Input</Direction>
        <IsCore>true</IsCore>
        <Name>EndpointAddress</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>