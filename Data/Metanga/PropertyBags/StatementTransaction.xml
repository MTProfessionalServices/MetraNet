<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            Collection of transactions, which contains invoices and payments issued during the specific time range
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>StatementTransaction</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>StatementTransaction</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Transaction date.  For invoice this would be the invoice date
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Date</Name>
        <Required>true</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            InvoiceId or PaymentId. This is the Metanga internal id
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Id</Name>
        <Required>true</Required>
        <Type>
          <BaseType>UniqueIdentifier</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            InvoiceSeed or empty for payment
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Number</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>255</Length>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Invoice or Payment
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>StatementTransactionType</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Name>StatementTransactionType</Name>
          <PropertyBagMode>PropertyBag</PropertyBagMode>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Amount of Invoice. Empty fot Payment
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>AmountCharged</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Decimal</BaseType>
          <ListType>None</ListType>
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureQualifier i:nil="true" />
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Amount of Payment. Empty for Invoice
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>AmountPaid</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Decimal</BaseType>
          <ListType>None</ListType>
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureQualifier i:nil="true" />
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Running balance since statement start date
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Balance</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Decimal</BaseType>
          <ListType>None</ListType>
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureQualifier i:nil="true" />
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            Empty for Now
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>DueDate</Name>
        <Required>false</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>