<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" xmlns="MetraTech">
  <DefaultValue i:nil="true" />
  <Description>
            Defines a product id and its capacity within a given invoice
            </Description>
  <Direction>Input</Direction>
  <IsCore>true</IsCore>
  <Name>Tier</Name>
  <Required>false</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>Tier</Name>
    <PropertyBagMode>PropertyBag</PropertyBagMode>
  </Type>
  <Properties>
    <Properties>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The product resolved by this tier
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Product</Name>
        <Required>true</Required>
        <Type i:type="PropertyBagType">
          <BaseType>PropertyBag</BaseType>
          <ListType>None</ListType>
          <Name>Product</Name>
          <PropertyBagMode>ExtensibleEntity</PropertyBagMode>
        </Type>
      </Property>
      <Property>
        <DefaultValue i:nil="true" />
        <Description>
            The maximum capacity for this tier
            </Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Capacity</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Decimal</BaseType>
          <ListType>None</ListType>
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureQualifier i:nil="true" />
        </Type>
      </Property>
    </Properties>
  </Properties>
</PropertyBag>