<?xml version="1.0" encoding="utf-8"?>
<PropertyBag xmlns:arrays="http://schemas.microsoft.com/2003/10/Serialization/Arrays" xmlns:i="http://www.w3.org/2001/XMLSchema-instance" i:type="ServiceDefinitionEntity" xmlns="MetraTech">
  <Availability>Unknown</Availability>
  <DefaultValue i:nil="true" />
  <Description>This service definition will be used to update the properties of a DNIS (a.k.a. DDI) number. Changes to the properties will be effective dated.</Description>
  <Direction>Input</Direction>
  <IsCore>false</IsCore>
  <Name>dnistable</Name>
  <Required>true</Required>
  <Type i:type="PropertyBagType">
    <BaseType>PropertyBag</BaseType>
    <ListType>None</ListType>
    <Name>ServiceDefinition</Name>
    <PropertyBagMode>ExtensibleEntity</PropertyBagMode>
  </Type>
  <Namespace>premconf.com.ServiceDefinitions</Namespace>
  <Properties>
    <Properties>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The internal MetraNet account identifier</Description>
        <Direction>Input</Direction>
        <IsCore>true</IsCore>
        <Name>AccountId</Name>
        <Required>true</Required>
        <Type i:type="NumberType">
          <BaseType>Integer32</BaseType>
          <ListType>None</ListType>
          <FixedUnitOfMeasure i:nil="true" />
          <UnitOfMeasureCategory i:nil="true" />
          <UnitOfMeasureMode>None</UnitOfMeasureMode>
          <UnitOfMeasureProperty i:nil="true" />
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The time the event is deemed to have occurred</Description>
        <Direction>Input</Direction>
        <IsCore>true</IsCore>
        <Name>Timestamp</Name>
        <Required>true</Required>
        <Type>
          <BaseType>DateTime</BaseType>
          <ListType>None</ListType>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The DNIS (DDI) number being updated. If an unknown DNIS number is received, it gets inserted with effective date = today. Otherwise, the new properties are made effective starting today.</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>DNISDigits</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>50</Length>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The telco that carries the connection to this DNIS</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Carrier</Name>
        <Required>true</Required>
        <Type i:type="EnumerationType">
          <BaseType>Enumeration</BaseType>
          <ListType>None</ListType>
          <Category>premconf.com/Conferencing.Carrier</Category>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The country where the other end of the DNIS connection ends. This is the country to which the participant is dialing. The participant may or may not be calling from the same country.</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>CountryOrigin</Name>
        <Required>true</Required>
        <Type i:type="EnumerationType">
          <BaseType>Enumeration</BaseType>
          <ListType>None</ListType>
          <Category>premconf.com/accountcreation.CountryCode</Category>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The city / region on which the DNIS number ends.</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Locality</Name>
        <Required>true</Required>
        <Type i:type="StringType">
          <BaseType>String</BaseType>
          <ListType>None</ListType>
          <Length>46</Length>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The transport type used for this DNIS number.</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>Transport</Name>
        <Required>true</Required>
        <Type i:type="EnumerationType">
          <BaseType>Enumeration</BaseType>
          <ListType>None</ListType>
          <Category>premconf.com/Conferencing.Transport</Category>
        </Type>
      </Property>
      <Property i:type="ServiceDefinitionProperty">
        <Availability>Unknown</Availability>
        <DefaultValue i:nil="true" />
        <Description>The service level used for this DNIS number.</Description>
        <Direction>Input</Direction>
        <IsCore>false</IsCore>
        <Name>ServiceLevel</Name>
        <Required>true</Required>
        <Type i:type="EnumerationType">
          <BaseType>Enumeration</BaseType>
          <ListType>None</ListType>
          <Category>premconf.com/Conferencing.PremServiceLevel</Category>
        </Type>
      </Property>
    </Properties>
  </Properties>
  <AutoDelete>false</AutoDelete>
  <AutoResubmit>false</AutoResubmit>
</PropertyBag>