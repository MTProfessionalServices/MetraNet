  <ArgumentListType>Fixed</ArgumentListType>
  <FixedArguments>  
    <Argument>
      <Name>Number</Name>
      <DataType>decimal</DataType>
      <Description>The number that you want to find the feiling for.</Description>
    </Argument>
  </FixedArguments>
