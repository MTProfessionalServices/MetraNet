<TreeLogicFunction>
  <Name>Floor</Name>
  <Category>Math</Category>
  <ReturnType>Decimal</ReturnType>
  <Description>Returns the smallest integer greater than or equal to the specified number.</Description>
  <ArgumentListType>Fixed</ArgumentListType>
  <FixedArguments>
    <Argument>
      <Name>Number</Name>
      <DataType>decimal</DataType>
      <Description>The number the you want to find the floor for.</Description>
    </Argument>
  </FixedArguments>
  <Mtsql>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate>floor({0})</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </Mtsql>
