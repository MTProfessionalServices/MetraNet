<?xml version="1.0" encoding="utf-8"?>
<TreeLogicFunction>
  <Name>Max</Name>
  <Category>Math</Category>
  <ReturnType>decimal</ReturnType>
  <Description>Returns the largest value.</Description>
  <ArgumentListType>Variable</ArgumentListType>
  <VariableArgumentTemplate>
    <Name>Number</Name>
    <DataType>Decimal</DataType>
    <Description>One of the numbers for which you want to find a maximum.</Description>
  </VariableArgumentTemplate>
  <Mtsql>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate></CodeGenTemplate>
    <CodeGenMode>Custom</CodeGenMode>
    <Inlineable>False</Inlineable>
  </Mtsql>
  <CSharp>
    <FirstSupported>6.0.5</FirstSupported>
    <CodeGenTemplate>Math.Max({0}, {1})</CodeGenTemplate>
    <CodeGenMode>RecursiveTemplate</CodeGenMode>
  </CSharp>
</TreeLogicFunction>