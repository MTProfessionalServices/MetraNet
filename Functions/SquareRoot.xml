<?xml version="1.0" encoding="utf-8"?>
<TreeLogicFunction>
  <Name>SquareRoot</Name>
  <Category>Math</Category>
  <ReturnType>decimal</ReturnType>
  <Description>Returns the square root of the specified number.</Description>
  <ArgumentListType>Fixed</ArgumentListType>
  <FixedArguments>
    <Argument>
      <Name>Number</Name>
      <DataType>decimal</DataType>
      <Description>The number that you want to take the square root of.</Description>
    </Argument>
  </FixedArguments>
  <Mtsql>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate>sqrt({0})</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </Mtsql>
  <CSharp>
    <FirstSupported>6.0.5</FirstSupported>
    <CodeGenTemplate>Math.Sqrt({0})</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </CSharp>
</TreeLogicFunction>
