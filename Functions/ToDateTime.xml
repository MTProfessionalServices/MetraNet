<?xml version="1.0" encoding="utf-8"?>
<TreeLogicFunction>
  <Name>ToDateTime</Name>
  <Category>DateTime</Category>
  <ReturnType>DateTime</ReturnType>
  <Description>Converts the value to a DateTime.</Description>
  <ArgumentListType>Fixed</ArgumentListType>
  <FixedArguments>
    <Argument>
      <Name>Value</Name>
      <DataType>any</DataType>
      <Description>The value that you want to convert to datetime.</Description>
    </Argument>
  </FixedArguments>
  <Mtsql>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate>CAST({0} AS DATETIME)</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </Mtsql>
  <CSharp>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate>({0}).ToDateTime()</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </CSharp>
</TreeLogicFunction>