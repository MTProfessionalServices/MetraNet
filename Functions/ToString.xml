<?xml version="1.0" encoding="utf-8"?>
<TreeLogicFunction>
  <Name>ToString</Name>
  <Category>Text</Category>
  <ReturnType>string</ReturnType>
  <Description>Converts the value to a string.</Description>
  <ArgumentListType>Fixed</ArgumentListType>
  <FixedArguments>
    <Argument>
      <Name>Value</Name>
      <DataType>any</DataType>
      <Description>The value that you want to convert to a string.</Description>
    </Argument>
  </FixedArguments>
  <Mtsql>
    <FirstSupported>5.0</FirstSupported>
    <CodeGenTemplate>CAST({0} AS VARCHAR)</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </Mtsql>
  <CSharp>
    <FirstSupported>6.0.5</FirstSupported>
    <CodeGenTemplate>({0}).ToString()</CodeGenTemplate>
    <CodeGenMode>BasicTemplate</CodeGenMode>
  </CSharp>
</TreeLogicFunction>