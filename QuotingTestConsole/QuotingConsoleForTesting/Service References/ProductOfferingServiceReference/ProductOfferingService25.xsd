      <xs:enumeration value="MOM" />
      <xs:enumeration value="MCM" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="LoginApplication" nillable="true" type="tns:LoginApplication" />
  <xs:simpleType name="AuthenticationType">
    <xs:restriction base="xs:string">
      <xs:enumeration value="MetraNetInternal" />
      <xs:enumeration value="ActiveDirectory" />
    </xs:restriction>
  </xs:simpleType>
  <xs:element name="AuthenticationType" nillable="true" type="tns:AuthenticationType" />
