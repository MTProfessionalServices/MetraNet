<?xml version="1.0" encoding="UTF-8"?>
<xmlconfig>
    <AccountType>
        <Name>AllTypes</Name>
        <Description>This is compound account type which should include all available account views</Description>
        <ServiceDefinitions>
            <ServiceDef>
                <Operation>metratech.com/accountcreation/operation/Add</Operation>
                <Name>metratech.com/accountcreation</Name>
            </ServiceDef>
        </ServiceDefinitions>
        <CanBePayer>False</CanBePayer>
        <CanSubscribe>True</CanSubscribe>
        <CanHaveSyntheticRoot>False</CanHaveSyntheticRoot>
        <CanParticipateInGSub>True</CanParticipateInGSub>
        <CanHaveTemplates>True</CanHaveTemplates>
        <IsVisibleInHierarchy>False</IsVisibleInHierarchy>
        <IsCorporate>False</IsCorporate>
        <AccountViews>
            <AdapterSet>
                <Name>Internal</Name>
                <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
                <ConfigFile>metratech.com/internal</ConfigFile>
            </AdapterSet>
            <AdapterSet>
                <Name>LDAP</Name>
                <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
                <ConfigFile>metratech.com/contact</ConfigFile>
            </AdapterSet>
        </AccountViews>
        <DirectDescendentAccountTypes></DirectDescendentAccountTypes>
        <DirectAncestorAccountTypes>
            <Ancestor>
                <Name>CorporateAccount</Name>
            </Ancestor>
            <Ancestor>
                <Name>CoreSubscriber</Name>
            </Ancestor>
            <Ancestor>
                <Name>IndependentAccount</Name>
            </Ancestor>
            <Ancestor>
                <Name>Root</Name>
            </Ancestor>
            <Ancestor>
                <Name>AllTypes</Name>
            </Ancestor>
            <Ancestor>
                <Name>DepartmentAccount</Name>
            </Ancestor>
        </DirectAncestorAccountTypes>
    </AccountType>
</xmlconfig>