<?xml version="1.0" encoding="UTF-8" ?>
<xmlconfig>
  <AccountType>
    <Name>IndependentAccount</Name>
    <Description>This is an example of an independent account.</Description>
    <ServiceDefinitions>
      <ServiceDef>
        <Operation>metratech.com/accountcreation/operation/Add</Operation>
        <Name>metratech.com/AccountCreation</Name>
      </ServiceDef>
      <ServiceDef>
        <Operation>metratech.com/accountcreation/operation/Update</Operation>
        <Name>metratech.com/AccountCreation</Name>
      </ServiceDef>
    </ServiceDefinitions>
    <CanBePayer>True</CanBePayer>
    <CanSubscribe>True</CanSubscribe>
    <CanHaveSyntheticRoot>False</CanHaveSyntheticRoot>
    <CanParticipateInGSub>False</CanParticipateInGSub>
    <CanHaveTemplates>False</CanHaveTemplates>
    <IsVisibleInHierarchy>False</IsVisibleInHierarchy>
    <IsCorporate>False</IsCorporate>
    <AccountViews>
      <AdapterSet>
        <Name>Internal</Name>
        <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
        <ConfigFile>metratech.com/internal</ConfigFile>
      </AdapterSet>
      <AdapterSet>
        <Name>LDAP</Name>
        <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
        <ConfigFile>metratech.com/contact</ConfigFile>
      </AdapterSet>
    </AccountViews>
    <DirectDescendentAccountTypes>
		  <Descendent>
  	    <Name></Name>
  	  </Descendent>
    </DirectDescendentAccountTypes>
  </AccountType>
</xmlconfig>