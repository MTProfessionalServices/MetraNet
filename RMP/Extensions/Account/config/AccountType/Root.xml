<?xml version="1.0" encoding="UTF-8"?>
<xmlconfig>
	<AccountType>
		<Name>Root</Name>
		<Description>This is the root of the account hierarchy.  Only one account of root can exist.  Don't customize.</Description>
		<ServiceDefinitions>
			<ServiceDef>
				<Operation></Operation>
				<Name></Name>
			</ServiceDef>
		</ServiceDefinitions>
		<CanBePayer>False</CanBePayer>
		<CanSubscribe>False</CanSubscribe>
		<CanHaveSyntheticRoot>False</CanHaveSyntheticRoot>
		<CanParticipateInGSub>False</CanParticipateInGSub>
		<CanHaveTemplates>False</CanHaveTemplates>
		<IsVisibleInHierarchy>True</IsVisibleInHierarchy>
		<IsCorporate>False</IsCorporate>
		<AccountViews>
			<AdapterSet>
				<Name>Internal</Name>
				<ProgID>MTAccount.MTSQLAdapter.1</ProgID>
				<ConfigFile>metratech.com/internal</ConfigFile>
			</AdapterSet>
		</AccountViews>
		<DirectDescendentAccountTypes>
			<Descendent>
				<Name>IndependentAccount</Name>
			</Descendent>
			<Descendent>
				<Name>SystemAccount</Name>
			</Descendent>
			<Descendent>
				<Name>CorporateAccount</Name>
			</Descendent>
		</DirectDescendentAccountTypes>
	</AccountType>
</xmlconfig>
