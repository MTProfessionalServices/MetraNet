<?xml version="1.0" encoding="UTF-8" ?>
<xmlconfig>
  <AccountType>
    <Name>SystemAccount</Name>
    <Description>This is the system account type, used for su, csr, ops accounts</Description>
    <ServiceDefinitions>
      <ServiceDef>
        <Operation>metratech.com/accountcreation/operation/Add</Operation>
        <Name>metratech.com/SystemAccountcreation</Name>
      </ServiceDef>
      <ServiceDef>
        <Operation>metratech.com/accountcreation/operation/Update</Operation>
        <Name>metratech.com/SystemAccountcreation</Name>
      </ServiceDef>
    </ServiceDefinitions>
    <CanBePayer>True</CanBePayer>
    <CanSubscribe>False</CanSubscribe>
    <CanHaveSyntheticRoot>False</CanHaveSyntheticRoot>
    <CanParticipateInGSub>False</CanParticipateInGSub>
    <CanHaveTemplates>False</CanHaveTemplates>
    <IsVisibleInHierarchy>True</IsVisibleInHierarchy>
    <IsCorporate>False</IsCorporate>
    <AccountViews>
      <AdapterSet>
        <Name>Internal</Name>
        <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
        <ConfigFile>metratech.com/internal</ConfigFile>
      </AdapterSet>
      <AdapterSet>
        <Name>LDAP</Name>
        <ProgID>MTAccount.MTSQLAdapter.1</ProgID>
        <ConfigFile>metratech.com/contact</ConfigFile>
      </AdapterSet>
    </AccountViews>
    <DirectDescendentAccountTypes>
		  <Descendent>
  	    <Name>SystemAccount</Name>
  	  </Descendent>    
    </DirectDescendentAccountTypes>
  </AccountType>
</xmlconfig>