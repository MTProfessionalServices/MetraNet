<xmlconfig>
 <AuthUserName>su</AuthUserName>
  <AuthPassword>su123</AuthPassword>
  <AuthNamespace>system_user</AuthNamespace>
  <SerializeContext ptype="BOOLEAN">FALSE</SerializeContext>
  <session>
    <ServiceName>metratech.com/AccountCreation</ServiceName>

    <inputs>

      <actiontype>account</actiontype>

      <!-- Required for account table creation plugin -->
      <!-- User Name -->
      <username>monthly</username>

      <!-- Password -->
      <password_>123456</password_>

      <!-- Name_Space -->
      <name_space>MT</name_space>

      <!-- Language -->
      <language>US</language>

      <billable>T</billable>
     <currency>USD</currency>
      <!-- Day of month that the user wants to be billed on -->
      <dayofmonth ptype="INTEGER">22</dayofmonth>

      <!-- Timezone ID -->
      <timezoneID>18</timezoneID>

      <!-- Timezone Offset -->
      <timezoneoffset>-3.5</timezoneoffset>

      <!-- Tax Exempt -->
      <taxexempt>F</taxexempt>

      <!-- City -->
      <city>Waltham</city>

      <!-- State -->
      <state>MA</state>

      <!-- Zip -->
      <zip>02452</zip>

       <!-- Account type-->
      <accounttype>IndependentAccount</accounttype>

      
      <!-- Usage Cycle Type enum type, set to on-demand=1 -->
      <usagecycletype>Monthly</usagecycletype>

      <!-- Payment Method (none or creditcard) -->
      <paymentmethod>CashOrCheck</paymentmethod>

      <!-- Transaction cookie -->
      <transactioncookie></transactioncookie>	      

      <!-- Account created -->
      <!-- <accountcreated>N</accountcreated> -->

      <!-- Operation -->
      <operation>Add</operation>	      

      <!-- Required for LDAP creation plugin -->
      <contacttype>bill-to</contacttype>
      <!-- First Name -->
      <firstname>autosdk</firstname>

      <!-- Last Name -->
      <lastname>Account</lastname>

      <!-- Email -->
      <email>autosdk@metratech.com</email>

      <!-- Phone Number -->
      <phonenumber>212 555 1212</phonenumber>

      <!-- Account ID -->
      <_accountid>0</_accountid>

      <!-- Company -->
      <company>MetraTech</company>

      <address1>330 Bear Hill Road</address1>
      <address2></address2>
      <address3></address3>
      <country>USA</country>
      <middleinitial>M</middleinitial>
      <facsimiletelephonenumber>6172532500</facsimiletelephonenumber>

      <accountstartdate ptype="DATETIME">1999-12-5T14:20:07Z</accountstartdate>
      <accountenddate ptype="DATETIME">2000-1-4T14:20:07Z</accountenddate>
      <accountstatus>AC</accountstatus>
      <StatusReason>0</StatusReason>
     </inputs>

  </session>
</xmlconfig>

