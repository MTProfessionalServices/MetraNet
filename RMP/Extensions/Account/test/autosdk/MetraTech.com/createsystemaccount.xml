<xmlconfig>

  <AuthUserName>su</AuthUserName>
  <AuthPassword>su123</AuthPassword>
  <AuthNamespace>system_user</AuthNamespace>
  <SerializeContext ptype="BOOLEAN">FALSE</SerializeContext>

  <session>
    <ServiceName>metratech.com/SystemAccountCreation</ServiceName>
    <inputs>
     

      <!-- 0 for Account, 1 for Contact, 2 for Both -->
      <ActionType>2</ActionType>

      <_AccountID>0</_AccountID>

      <UserName>newsys2</UserName>
 
      <!-- "123" encrypted is "D0myR1j+gzM=". -->
      <Password_>123</Password_>

      <Name_Space>system_user</Name_Space>

      <!-- US is 0, DE is 1, FR is 3, IT is 4, JP is 5-->
      <Language>0</Language>

      <AccountType>SystemAccount</AccountType>

	 <!-- Usage Cycle Type enum type, set to on-demand=1 -->
      <usagecycletype>Monthly</usagecycletype>

      <!-- Payment Method (none or creditcard) -->
      <paymentmethod>CashOrCheck</paymentmethod>

    <!-- Day of month that the user wants to be billed on -->
      <dayofmonth ptype="INTEGER">1</dayofmonth>

      <!-- (GMT-05:00) Eastern Time (US & Canada) -->
      <TimezoneId>18</TimezoneId>
      <TimezoneOffset>5</TimezoneOffset>
      <currency>USD</currency>
      <!-- 0 for Add, 1 for Update, 2 for Delete -->
      <Operation>0</Operation>

      <AccountStartDate ptype="DATETIME">2004-10-25T14:20:07Z</AccountStartDate>

      <AccountEndDate ptype="DATETIME">2006-12-31T14:20:07Z</AccountEndDate>

      <AccountStatus>AC</AccountStatus>

     <LoginApplication>CSR</LoginApplication>
      <ApplyDefaultSecurityPolicy>T</ApplyDefaultSecurityPolicy>
  
    </inputs>
  </session>

</xmlconfig>
