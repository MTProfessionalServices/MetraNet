<mt_config>
  <enum_spaces>
    <enum_space  name="metratech.com/audioconfcommon">
    <description>Common audio conference enum types</description>
    <enums>
       <enum name="ServiceLevel">
		<description>Service Level</description>
			<entries>
			<entry name="Basic">
			   <value>1</value>
			</entry>
			<entry name="Standard">
			<value>2</value>
			</entry>
			<entry name="Executive">
			<value>3</value>
			</entry>
			<entry name="Premiere">
			<value>4</value>
			</entry>
			<entry name="Automated">
			<value>5</value>
			</entry>
	</entries>
	</enum>
  <enum name="ChargeType">
		<description>Charge Type - Fixed or PerPort</description>
			<entries>
			<entry name="Fixed">
				<value>0</value>
			</entry>
			<entry name="PerPort">
				<value>1</value>
			</entry>
			</entries>
	</enum>
	<enum name="PortChargeDef">
		<description>Algorithm definition for port charges</description>
			<entries>
			<entry name="Simple">
				<value>0</value>
			</entry>
			<entry name="Threshold">
				<value>1</value>
			</entry>
			<entry name="Advanced">
				<value>2</value>
			</entry>
			</entries>
	</enum>

	<enum name="RoundMethod">
		<description>Rounding Method - Up or Down</description>
			<entries>
				<entry name="CEILING">
			   		<value>0</value>
				</entry>
				<entry name="FLOOR">
			   		<value>1</value>
				</entry>
			</entries>
	</enum>	
	</enums>
  </enum_space>
 </enum_spaces>
</mt_config>
