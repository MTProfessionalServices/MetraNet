<?xml version="1.0" encoding="UTF-8"?>

<xmlconfig>
  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <processor>
      <name>GetApplicFlags</name>
      <progid>MetraPipeline.PCRate.1</progid>
      <description>Look up minimum charge, unused/overused port charge, cancellation charge and reservation charge applicability rules for this conference call.
</description>

      <configdata>

        <!-- lookup from a named parameter table -->
        <RateLookup>
          <ParamTable>metratech.com/cancelapplic</ParamTable>
        </RateLookup>

       <RateLookup>
          <ParamTable>metratech.com/overusedportapplic</ParamTable>
        </RateLookup>

       <RateLookup>
          <ParamTable>metratech.com/reservationapplic</ParamTable>
        </RateLookup>

       <RateLookup>
          <ParamTable>metratech.com/unusedportapplic</ParamTable>
        </RateLookup>

       <RateLookup>
          <ParamTable>metratech.com/minapplic</ParamTable>
        </RateLookup>

       <!-- lookup from a dynamically determined parameter table -->
<!--
        <RateLookup>
          <ParamTableID>PTID</ParamTableID>
        </RateLookup>
-->
        <!-- Initialize the parameter table cache when this
             plug-in is initially loaded.

             This should boost performance if you have a lot of
             rate schedules that are being accessed only a few
             times, such as with ICB (Individual Case Based) rates.
         -->
      <InitParamTableCache ptype="BOOLEAN">TRUE</InitParamTableCache>

      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
