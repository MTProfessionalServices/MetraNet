<xmlconfig>
	<mtsysconfigdata>
		<effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
		<timeout ptype="INTEGER">30</timeout>
		<configfiletype>CONFIG_DATA</configfiletype>
	</mtsysconfigdata>

	<mtconfigdata>
		<version ptype="INTEGER">1</version>
		<processor>
			<name>SetTimestamp</name>
			<progid>MetraPipeline.SetCompoundProp.1</progid>
			<description>
			Used to set a property for the entire compound, based on a min or max operation
			on a parent or child property.
			</description>
			 <configdata>
					<TargetPropertyName>_Timestamp</TargetPropertyName>
					<Type>Datetime</Type>
					<Operation>MIN</Operation>
					<MutexName>SetCompoundProp</MutexName>
					<Sources>
						<Source>
							<ServiceDefName>metratech.com/audioconfconnection</ServiceDefName>
							<SourcePropertyName>ConnectTime</SourcePropertyName>
						</Source>
						<Source>
							<ServiceDefName>metratech.com/audioconfcall</ServiceDefName>
							<SourcePropertyName>ScheduledStartTime</SourcePropertyName>
						</Source>
						<Source>
							<ServiceDefName>metratech.com/audioconffeature</ServiceDefName>
							<SourcePropertyName>StartTime</SourcePropertyName>
						</Source>
					</Sources>
			</configdata>
		</processor>
	</mtconfigdata>
</xmlconfig>
