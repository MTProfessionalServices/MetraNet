<xmlconfig>
  <version ptype="INTEGER">1</version>
  <!-- Mediated audio conferencing -->
  <description>Stage for processing the charges for the overall conference call.</description>
  <stage>
    <name>AudioConfCall</name>
    <startstage ptype="BOOLEAN">YES</startstage>
    <finalstage ptype="BOOLEAN">NO</finalstage>
    <nextstage>WriteProductView</nextstage>
    <!-- Files generated from the UI:
       CancelApplic
       unusedportapplic
	 overusedportapplic
       reservationapplic
       cancelcharges
       unusedportdefinition
	 overusedportdefinition
  -->
    <dependencies>
      <dependency>
        <processor>piresolution</processor>
      </dependency>
      <dependency>
        <processor>namespace</processor>
        <dependson>piresolution</dependson>
      </dependency>
      <dependency>
        <processor>SetTimestamp</processor>
        <dependson>namespace</dependson>
      </dependency>
      <dependency>
        <processor>AccountResolution</processor>
        <dependson>SetTimestamp</dependson>
      </dependency>
      <dependency>
        <processor>instanceres</processor>
        <dependson>AccountResolution</dependson>
      </dependency>
      <dependency>
        <processor>notice</processor>
        <dependson>instanceres</dependson>
      </dependency>
      <dependency>
        <processor>PortChargeDef</processor>
        <dependson>notice</dependson>
      </dependency>
      <dependency>
        <processor>summation</processor>
        <dependson>PortChargeDef</dependson>
      </dependency>
      <dependency>
        <processor>CalcUnusedPorts</processor>
        <dependson>summation</dependson>
      </dependency>
      <dependency>
        <processor>GetApplicFlags</processor>
        <dependson>CalcUnusedPorts</dependson>
      </dependency>
      <dependency>
        <processor>CalcOverusedPorts</processor>
        <dependson>GetApplicFlags</dependson>
      </dependency>
      <dependency>
        <processor>getrates</processor>
        <dependson>CalcOverusedPorts</dependson>
      </dependency>
      <dependency>
        <processor>CalcUnusedPortCharges</processor>
        <dependson>getrates</dependson>
      </dependency>
      <dependency>
        <processor>leader</processor>
        <dependson>CalcUnusedPortCharges</dependson>
      </dependency>
      <dependency>
        <processor>CalcOverusedPortCharges</processor>
        <dependson>leader</dependson>
      </dependency>
      <dependency>
        <processor>CalcCancellationCharges</processor>
        <dependson>CalcOverusedPortCharges</dependson>
      </dependency>
      <dependency>
        <processor>CalcReservationCharges</processor>
        <dependson>CalcCancellationCharges</dependson>
      </dependency>
      <dependency>
        <processor>CalcTotalConfCharge</processor>
        <dependson>CalcReservationCharges</dependson>
      </dependency>
      <dependency>
        <processor>viewproduct</processor>
        <dependson>CalcTotalConfCharge</dependson>
      </dependency>
      <dependency>
        <processor>CalcRawDuration</processor>
        <dependson>viewproduct</dependson>
      </dependency>
      <dependency>
        <processor>CalcActualDuration</processor>
        <dependson>CalcRawDuration</dependson>
      </dependency>
      <dependson>CalcActualDuration</dependson>
    </dependencies>
  </stage>
  <possiblenextstages />
</xmlconfig>