<xmlconfig>

  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>


 <version ptype="INTEGER">1</version>
 <!-- First processor configuration -->
 <processor>
  <name>viewproduct</name>
  <progid>MTPipeline.PropGenProcessor.1</progid>
  <description>Sets the product ID and product view ID.</description>

  <!--
  <autotest>
    <file>vptest.xml</file>
  </autotest>
  -->

  <inputs>
  </inputs>

  <outputs>
    <output type="LONG">_ProductViewID</output>
    <output type="LONG">_ProductID</output>
  </outputs>

  <!-- Processor specific configuration data -->
  <configdata>
	<default_actions>
		<action>
			<prop_name>_ProductViewID</prop_name>
			<prop_value ptype="ID">metratech.com/audioConfCall</prop_value>
		</action>
		<action>
			<prop_name>_ProductID</prop_name>
			<prop_value ptype="ID">metratech.com/audioConfCall</prop_value>
		</action>
	</default_actions>
  </configdata>

 </processor>

 </mtconfigdata>

</xmlconfig>
