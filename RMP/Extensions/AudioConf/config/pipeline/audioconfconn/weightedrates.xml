<xmlconfig>
  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <!-- First processor configuration -->
    <processor>
      <name>WeightedRates</name>
      <progid>MetraPipeline.WeightedRate.1</progid>
      <description>Calculates the international surcharge rate.</description>

      <!-- Specifically this instance sets these properties -->
      <inputs>
        <input type="timestamp">ConnectTime</input>
        <input type="timestamp">DisconnectTime</input>
        <input type="int32">timezoneID</input>
        <input type="ENUM" enumspace="metratech.com/audioconfconnection" enumtype="CallType">CallType</input>
        <input type="ENUM" enumspace="metratech.com/audioconfconnection" enumtype="Mode">Mode</input>
        <input type="ENUM" enumspace="metratech.com/audioconfconnection" enumtype="Transport">Transport</input>
        <input type="ENUM" enumspace="global" enumtype="CountryName">CountryName</input>
      </inputs>
      <outputs>
        <output type="DECIMAL">BaseTransportRate</output>
        <output type="DECIMAL">BaseTransportSetupCharge</output>
        <output type="DECIMAL">BaseTransportMinCharge</output>
        <output type="ENUM" enumspace="metratech.com/audioconfconnection" enumtype="UOM">TransportMinUOM</output>
        <output type="DECIMAL">IntlRate</output>
        <output type="ENUM" enumspace="metratech.com/calendar" enumtype="CalendarCode">CalendarCode</output>
      </outputs>
      
      <!-- Processor specific configuration data -->
      <configdata>
        <rate_prop_name>BaseTransportRate</rate_prop_name>
        <rate_prop_name>IntlRate</rate_prop_name>
        <calendar_code_prop_name>CalendarCode</calendar_code_prop_name>
        <SetEnumCalendarCode ptype="BOOL">TRUE</SetEnumCalendarCode>
        <calendar_code_enum_space>metratech.com/calendar</calendar_code_enum_space>
        <calendar_code_enum_type>CalendarCode</calendar_code_enum_type>
        <start_time_prop_name>ConnectTime</start_time_prop_name>
        <end_time_prop_name>DisconnectTime</end_time_prop_name>
        <timezoneid_prop_name>timezoneID</timezoneid_prop_name>
        <stage_name>AudioConfConn</stage_name>
        <execute_plug_in>TransportIntlRate</execute_plug_in>

        <!-- this is the same calendar that weightedconn uses -->
        <calendar>calendar.xml</calendar>
        <calendar_id_prop_name>calendar_id</calendar_id_prop_name>
      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
