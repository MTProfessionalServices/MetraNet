<xmlconfig>
  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>

 <version ptype="INTEGER">1</version>
 <!-- First processor configuration -->
 <processor>
  <name>CalcMTI</name>
  <progid>MetraPipeline.MTSQLInterpreter.1</progid>
  <description>Calculates MTI in seconds</description>


  <inputs>
  	<input type="INTEGER">MTI</input>
  	<input type="INTEGER">UOMNUM</input>
  </inputs>
  <outputs>
  	<output type="INTEGER">MTIInSeconds</output>
  </outputs>
  
  <!-- Processor specific configuration data -->
  <configdata>
 
  <Program><![CDATA[
		
CREATE PROCEDURE uomnum 
  @MTI INTEGER 
  @UOMNUM INTEGER 
  @MTIInSeconds INTEGER OUTPUT
AS
	SET @MTIInSeconds = @MTI * @UOMNUM;
	 
  ]]></Program> 
    
  </configdata>

 </processor>

  </mtconfigdata>

</xmlconfig>

