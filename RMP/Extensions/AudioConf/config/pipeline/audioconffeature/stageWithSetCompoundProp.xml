<!-- use this version for testing the new SetCompoundProp plugin -->
<xmlconfig>
 <version ptype="INTEGER">1</version>
 <stage>
  <name>audioconffeature</name>
  <startstage ptype="BOOLEAN">YES</startstage>
  <finalstage ptype="BOOLEAN">NO</finalstage>
  <nextstage>WriteProductView</nextstage>

  <!--
  <routefrom>
    <queue>RoutingQueue</queue>
  </routefrom>
  -->

  <!--
  <maxsetsize ptype="INTEGER">20</maxsetsize>
  <maxwaittime ptype="INTEGER">10</maxwaittime>
  -->

  <!-- files generated from the UI:
       RateConnections.xml
       RateInternational.xml
  -->

  <dependencies>
   <dependson>viewproduct</dependson>
   <dependson>instanceres</dependson>
   <dependson>calcduration</dependson>
  
  <dependency>    
		<processor>CalcMTI</processor>    
    <dependson>UOMNUM</dependson>  
  </dependency>
  
  <dependency>
    <processor>CalcAmount</processor>
    <dependson>CalcTimeBasedCharge</dependson>
    <dependson>CalcMetricCharge</dependson>
    <dependson>GetAllRates</dependson>
  </dependency>


  <dependency>
    <processor>CalcTimeBasedCharge</processor>
    <dependson>CalcRoundedDuration</dependson>
    <dependson>RUOMNUM</dependson>
    <dependson>GetAllRates</dependson>
  </dependency>

  
  <dependency>
    <processor>CalcMetricCharge</processor>
    <dependson>GetAllRates</dependson>
  </dependency>
  
  
  <dependency>
    <processor>CalcRoundedDuration</processor>
    <dependson>CalcDuration</dependson>
    <dependson>CalcMTI</dependson>
  </dependency>
  
  <dependency>
    <processor>RUOMNUM</processor>
    <dependson>GetAllRates</dependson>
  </dependency>

	<dependency>
    <processor>UOMNUM</processor>
    <dependson>GetAllRates</dependson>
  </dependency>
  
  <dependency>
    <processor>GetAllRates</processor>
    <dependson>instanceres</dependson>
  </dependency>
   
  <dependency>
    <processor>instanceres</processor>
    <dependson>piresolution</dependson>
    <dependson>accountresolution</dependson>
  </dependency>

  <dependency>
    <processor>accountresolution</processor>
    <dependson>namespace</dependson>
    <dependson>SetTimestamp</dependson>
  </dependency>

  </dependencies>
 </stage>
</xmlconfig>

