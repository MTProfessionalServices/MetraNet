<xmlconfig>

  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>


 <version ptype="INTEGER">1</version>
 <!-- First processor configuration -->
 <processor>
  <name>ViewProduct</name>
  <progid>MTPipeline.PropGenProcessor.1</progid>
  <description>Set the product view ID and product ID</description>

  
  <!-- Processor specific configuration data -->
  <configdata>
	<default_actions>
		<action>
			<prop_name>_ProductViewID</prop_name>
			<prop_value ptype="INTEGER">0</prop_value>
		</action>
		<action>
			<prop_name>_ProductID</prop_name>
			<prop_value ptype="INTEGER">0</prop_value>
		</action>
	</default_actions>


	<constraint_set>
		<actions>
			<action>
				<prop_name>_ProductViewID</prop_name>
				<prop_value ptype="ID">metratech.com/audioConfFeature</prop_value>
			</action>
			<action>
				<prop_name>_ProductID</prop_name>
				<prop_value ptype="ID">metratech.com/audioConfFeature</prop_value>
			</action>
		</actions>

		<constraint>
			<prop_name>_ServiceID</prop_name>
			<condition>equal</condition>
			<prop_value ptype="ID">metratech.com/audioConfFeature</prop_value>
		</constraint>
	</constraint_set>


  </configdata>

 </processor>

 </mtconfigdata>

</xmlconfig>
