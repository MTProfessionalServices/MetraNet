<xmlconfig>
  <BillSoft>
    <!-- This is the  base path to the EZTax installation. -->
    <EZTaxInstallPath>C:\BillSoft\EZTax</EZTaxInstallPath>
    <!-- This is the directory where audit logs should be placed. -->
    <EZTaxLogPath>C:\BillSoft\AuditFiles\</EZTaxLogPath>
    <!-- This is the prefix that is prepended to every audit log. -->
    <EZTaxLogFilePrefix>EZTaxAuditLog</EZTaxLogFilePrefix>
  </BillSoft>
</xmlconfig>