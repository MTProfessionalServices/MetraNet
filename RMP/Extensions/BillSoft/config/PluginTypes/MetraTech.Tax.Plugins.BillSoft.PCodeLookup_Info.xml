<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
    <ProgID>MetraTech.Tax.Plugins.BillSoft.PCodeLookup</ProgID>
    <DisplayName>PCodeLookup</DisplayName>
    <Description>This plugin will take parameters and lookup a pcode using the BillSoft Package.</Description>
    <Author>Joseph Barnett</Author>
    <Category>Basic Usage</Category>
    <SupportsSessionSets>True</SupportsSessionSets>
    <TransactionalSupport>Supports</TransactionalSupport>
    <Editor>plgGenericPropertyGrid</Editor>
    <HelpURL></HelpURL>
    <IsCodeGenerated>False</IsCodeGenerated>
    <DeprecatedVersion DeprecatedMessage=""></DeprecatedVersion>
    <AutoPipelineBindingProperties>
        <ptype>
            <dn>CountryCode</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>Country Code</description>
            <xpath>PipelineBinding/CountryCode</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>StateAbbr</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>State Abbrviation</description>
            <xpath>PipelineBinding/StateAbbr</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>City</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>City</description>
            <xpath>PipelineBinding/City</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>ZipCode</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>Zip Code</description>
            <xpath>PipelineBinding/ZipCode</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>NPANXXNumber</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>NPANXXNumber</description>
            <xpath>PipelineBinding/NPANXXNumber</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>TaxPCodeValue</dn>
            <type>int64</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>Discovered PCode, or Zero if not able to resolve </description>
            <xpath>PipelineBinding/TaxPCodeValue</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
    </AutoPipelineBindingProperties>
    <AutoGeneralConfigProperties>
        <ptype>
            <dn>ResolveByZip</dn>
            <type>boolean</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>Resolove by zipcode</description>
            <xpath>GeneralConfig/ResolveByZip</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>ResolveByNPANXX</dn>
            <type>boolean</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>Resolve by NPANXX</description>
            <xpath>GeneralConfig/ResolveByNPANXX</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>EZTaxInstallPath</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>EZTax Install Path</description>
            <xpath>GeneralConfig/EZTaxInstallPath</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
    </AutoGeneralConfigProperties>
</xmlconfig>