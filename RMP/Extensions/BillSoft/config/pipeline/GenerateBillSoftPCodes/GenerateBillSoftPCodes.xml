<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
    <mtconfigdata>
        <version ptype="INTEGER">1</version>
        <processor>
            <name>GenerateBillSoftPCodes</name>
            <progid>MetraTech.Tax.Plugins.BillSoft.PCodeLookup</progid>
            <description>This plugin will resolve the pcode and output the pcode provided the input....</description>
            <configdata>
                <PipelineBinding>
                    <CountryCode>CountryCode</CountryCode>
                    <StateAbbr>StateAbbr</StateAbbr>
                    <City>Locality</City>
                    <ZipCode>ZipCode</ZipCode>
                    <NPANXXNumber>NPANXXNumber</NPANXXNumber>
                    <TaxPCodeValue>_SessionID</TaxPCodeValue>
                </PipelineBinding>
                <GeneralConfig>
                    <ResolveByZip>True</ResolveByZip>
                    <ResolveByNPANXX>False</ResolveByNPANXX>
					<EZTaxInstallPath>C:\BillSoft\EZTax\Data</EZTaxInstallPath>
                </GeneralConfig>
            </configdata>
        </processor>
    </mtconfigdata>
</xmlconfig>