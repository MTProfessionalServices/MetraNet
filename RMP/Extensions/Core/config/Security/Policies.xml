<principal type="role" name = "Junior Adjustment Manager">
<description>Adjustments - User: Authorizes a user to perform routine Adjustment Manager tasks in MetraCare
</description>
<csrassignable>y</csrassignable>
<subscriberassignable>n</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Application LogOn" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>MAM</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
  <compositecapability name="Apply Adjustments" guid="">
  <atomiccapabilities>
      <mtdecimalcapability>
        <op><![CDATA[<=]]></op>
        <value>100.00</value>
      </mtdecimalcapability>
      <mtenumtypecapability>
        <value>USD</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
	<compositecapability name="Impersonation" guid="">
	</compositecapability>
</compositecapabilities>
</policy>
</policies>
</principal>

