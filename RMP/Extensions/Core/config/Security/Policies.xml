<mt_config>
<principal type="role" name = "Super User">
<description>Super User: Authorizes full system access and permissions</description>
<csrassignable>y</csrassignable>
<subscriberassignable>n</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Unlimited Capability" guid="">
  </compositecapability>
</compositecapabilities>
</policy>
</policies>
</principal>

<principal type="role" name = "Product Administrator (MetraOffer)">
<description>Product Administrator: Authorizes a user to perform administrative tasks in MetraOffer</description>
<csrassignable>y</csrassignable>
<subscriberassignable>n</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Application LogOn" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>MCM</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
</compositecapabilities>
</policy>
</policies>
</principal>

<principal type="role" name = "Operations Staff (MetraControl)">
<description>Billing Systems Operator: Authorizes a user to perform routine tasks in MetraControl
</description>
<csrassignable>y</csrassignable>
<subscriberassignable>n</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Application LogOn" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>MOM</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
  <compositecapability name="Update Failed Transactions" guid="">
  </compositecapability>
</compositecapabilities>
</policy>
</policies>
</principal>

<principal type="role" name = "Subscriber (MetraView)">
<description>Subscriber: Authorizes a subscriber to perform routine tasks in MetraView
</description>
<csrassignable>y</csrassignable>
<subscriberassignable>y</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Application LogOn" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>MPS</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>

  <compositecapability name="Create Subscription" guid="">
  </compositecapability>
  <compositecapability name="Update Corporate Accounts" guid="">
  </compositecapability>
  <compositecapability name="Update subscriber accounts" guid="">
  </compositecapability>

  <compositecapability name="View Subscriptions" guid="">
  </compositecapability>

  <compositecapability name="Write Business Modeling Entities" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>Core</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>

  <compositecapability name="Read Business Modeling Entities" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>Core</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
</compositecapabilities>
</policy>
</policies>
</principal>

<principal type="role" name="Sales">
<description>Sales: Authorizes a sales associate to perform routine account ownership tasks in MetraCare
</description>
<csrassignable>y</csrassignable>
<subscriberassignable>n</subscriberassignable>
<policies>
<policy type="a">
<compositecapabilities>
  <compositecapability name="Application LogOn" guid="">
  <atomiccapabilities>
      <mtenumtypecapability>
        <value>MAM</value>
      </mtenumtypecapability>
    </atomiccapabilities>
  </compositecapability>
  <compositecapability name="View Online Bill" guid="">
  </compositecapability>
  <compositecapability name="Manage Owned Accounts" guid="">
    <atomiccapabilities>
      <mtenumtypecapability>
        <value>Write</value>
      </mtenumtypecapability>
      <mtpathcapability>
        <wildcard>*</wildcard>
        <value>/</value>
      </mtpathcapability>      
    </atomiccapabilities>  
  </compositecapability>
  <compositecapability name="View Subscriptions" guid="">
  </compositecapability>
  <compositecapability name="View Group Subscriptions" guid="">
  </compositecapability>  
</compositecapabilities>
</policy>
</policies>
</principal>

