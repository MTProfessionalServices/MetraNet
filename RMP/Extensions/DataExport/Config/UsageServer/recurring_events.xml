<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
    <ScheduledAdapters>
	<Adapter>
	<Name>QueueScheduledExportReports</Name>
	<DisplayName>Queue Scheduled Export Reports</DisplayName>
	<ClassID>MetraTech.DataExportFramework.Adapters.QueueScheduledReportsAdapter.QueueScheduledReportsAdapter, MetraTech.DataExportFramework.Adapters.QueueScheduledReportsAdapter</ClassID>
	<ConfigFile>QueueScheduledExportReports.xml</ConfigFile>
	<Description>This scheduled BPA kick off reports</Description>
      <RecurrencePattern>
		<SystemCycle>Daily</SystemCycle>        
      </RecurrencePattern>
		</Adapter>
    </ScheduledAdapters>

    <EndOfPeriodAdapters>
    <Adapter>
      <Name>QueueEOP</Name>
      <DisplayName>Queue EOP Export Reports</DisplayName>
      <ClassID>MetraTech.DataExportFramework.Adapters.EOPQueueReports.EOPQueueReports, MetraTech.DataExportFramework.Adapters.EOPQueueReports</ClassID>
      <ConfigFile>QueueEOPExportReports.xml</ConfigFile>
      <Description>Start Generating EOP Export Reports</Description>
      <RecurrencePattern>
        <UsageCycleType>All</UsageCycleType>
      </RecurrencePattern>
      <Dependencies>
        <DependsOn>Invoices</DependsOn>
      </Dependencies>
    </Adapter>
    </EndOfPeriodAdapters>

</xmlconfig>
