<?xml version="1.0" encoding="UTF-8"?>
<xmlconfig>
<AccountType>
	<Name>GSMServiceAccount</Name>
	<Description>This is a sample service account for a GSM Phone</Description>
<ServiceDefinitions>
		<ServiceDef>
			<Operation>metratech.com/AccountCreation/operation/Add</Operation>
			<Name>metratech.com/GSMCreateAndConnect</Name>
		</ServiceDef>
		<ServiceDef>
			<Operation>metratech.com/AccountCreation/operation/Update</Operation>
			<Name>metratech.com/GSMUpdate</Name>
		</ServiceDef>
		<ServiceDef>
			<Operation>metratech.com/GSMCreation/operation/Connect</Operation>
			<Name>metratech.com/GSMConnect</Name>
		</ServiceDef>
		<ServiceDef>
			<Operation>metratech.com/GSMCreation/operation/Create</Operation>
			<Name>metratech.com/GSMCreate</Name>
		</ServiceDef>
		<ServiceDef>
			<Operation>metratech.com/GSMCreation/operation/Disconnect</Operation>
			<Name>metratech.com/GSMDisconnect</Name>
		</ServiceDef>
	</ServiceDefinitions>
  <CanBePayer>False</CanBePayer>
  <CanSubscribe>True</CanSubscribe>
  <CanHaveSyntheticRoot>True</CanHaveSyntheticRoot>
  <CanParticipateInGSub>True</CanParticipateInGSub>
  <CanHaveTemplates>False</CanHaveTemplates>
  <IsVisibleInHierarchy>True</IsVisibleInHierarchy>
  <IsCorporate>False</IsCorporate>
	<AccountViews>
		<AdapterSet>
	    		<Name>Internal</Name>
	    		<ProgID>MTAccount.MTSQLAdapter.1</ProgID>
	    		<ConfigFile>metratech.com/internal</ConfigFile>
    		</AdapterSet>
		<AdapterSet>
	   		 <Name>GSM</Name>
	    		<ProgID>MTAccount.MTSQLAdapter.1</ProgID>
	    		<ConfigFile>metratech.com/GSM</ConfigFile>
    		</AdapterSet>
 	</AccountViews>
 	<DirectDescendentAccountTypes>
    <Descendent>
  	  <Name></Name>
  	</Descendent>
 	</DirectDescendentAccountTypes>
 	<DirectAncestorAccountTypes>
    <Ancestor>
  	  <Name>CoreSubscriber</Name>
  	</Ancestor>
    <Ancestor>
  	  <Name>Root</Name>
  	</Ancestor>
 	</DirectAncestorAccountTypes>
</AccountType>
</xmlconfig>
