<?xml version="1.0" encoding="utf-8"?>
<MenuManager xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema">
    <Menus>
        <Menu>
            <ID>TopMenu</ID>
            <Type>Horizontal</Type>
            <Description>Site top menu</Description>
      <MenuSections Configurable="false">
                <MenuSection>
                    <ID>TopMenu</ID>
           <Caption Configurable="false"></Caption>
                    <Description>Menu to appear in the upper right bill presentment site as a single section</Description>
                    <MenuItems>
                        <MenuItem>
                            <ID>Logout</ID>
                            <Caption>
                                <LocalizationTag>TEXT_LOGOUT</LocalizationTag>
                                <ResourceClassKey>MetraView/App_GlobalResources/TopMenu</ResourceClassKey>
                            </Caption>
                            <Link>[LogoutPage]</Link>
                            <Target></Target>
                            <Icon></Icon>
                            <Description>Logout option</Description>
                            <RequiredCapabilities></RequiredCapabilities>
                        </MenuItem>
                        <MenuItem>
                            <ID>Help</ID>
                            <Caption>
                                <LocalizationTag>TEXT_HELP</LocalizationTag>
                                <ResourceClassKey>MetraView/App_GlobalResources/TopMenu</ResourceClassKey>
                            </Caption>
                            <Link>JavaScript:Ext.UI.LoadHelp();</Link>
                            <Target></Target>
                            <Icon></Icon>
                            <Description>Help option</Description>
                            <RequiredCapabilities></RequiredCapabilities>
                        </MenuItem>
                    </MenuItems>
                </MenuSection>
            </MenuSections>
        </Menu>
    </Menus>
    <Name></Name>
    <Description></Description>
</MenuManager>