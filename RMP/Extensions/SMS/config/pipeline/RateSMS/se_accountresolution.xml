<xmlconfig>

  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <processor>
      <name>SE_AccountResolution</name>
      <progid>MetraPipeline.AccountResolution.1</progid>

      <condition>
       <![CDATA[
             CREATE PROCEDURE condition @_ExecutePlugin BOOLEAN @EndpointLogin VARCHAR(255)
  	    AS
	      SET @_ExecutePlugin = NOT @EndpointLogin IS NULL
 	    ]]>
      </condition>


      <configdata>


         <Resolution>
		<EndpointID>EndpointLogin</EndpointID>
		<EndpointNamespace>EndpointNamespace</EndpointNamespace>
		<CorporateID>CorporateLogin</CorporateID>
		<CorporateNamespace>CorporateNamespace</CorporateNamespace>
         </Resolution>

      <Retrieval>	
	   <Property>
	       <Name>AccountID</Name>
	       <InSession>_accountID</InSession>
	    </Property>

	    <Property>
	       <Name>PayingAccount</Name>
	       <InSession>_PayingAccount</InSession>
	    </Property>

	    <Property>
	       <Name>UsageCycleID</Name>
	       <InSession>_usageCycleID</InSession>
	    </Property>

	    <Property>
	       <Extension>core</Extension> 
	       <AccountAdapter>internal</AccountAdapter>
	       <Name>Pricelist</Name>
	       <InSession>_AccountPricelist</InSession>
	    </Property>

	    <Property>
	       <Extension>core</Extension>
	       <AccountAdapter>internal</AccountAdapter>
	       <Name>timezoneid</Name>
	       <InSession>timezoneid</InSession>
	    </Property>
      	    <Property>
              <Extension>core</Extension>
              <AccountAdapter>internal</AccountAdapter>
	      <Name>Currency</Name>
	      <InSession>_Currency</InSession>
	    </Property>
	 </Retrieval>	
      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
