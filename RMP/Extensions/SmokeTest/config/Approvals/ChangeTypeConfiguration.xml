<?xml version="1.0" encoding="utf-8"?>
<ChangeTypes>
  <ChangeTypeConfiguration>
    <Name>SampleUpdate</Name>
    <Description>Simple change type used for testing that logs changes but does not change the system</Description>
    <Enabled>true</Enabled>
    <AllowMoreThanOnePendingChange>true</AllowMoreThanOnePendingChange>
    <CapabilityRequiredForApproveOrDeny>Allow ApprovalsView</CapabilityRequiredForApproveOrDeny>
    <MethodForApply>
      <Assembly>MetraTech.Approvals.dll</Assembly>
      <Classname>MetraTech.Approvals.ChangeTypes.SampleUpdateChangeType</Classname>
    </MethodForApply>
    <MethodForSubmit>
      <Assembly>MetraTech.Approvals.dll</Assembly>
      <Classname>MetraTech.Approvals.ChangeTypes.SampleUpdateChangeType</Classname>
    </MethodForSubmit>
    <MethodForDeny>
      <Assembly>MetraTech.Approvals.dll</Assembly>
      <Classname>MetraTech.Approvals.ChangeTypes.SampleUpdateChangeType</Classname>
    </MethodForDeny>
  </ChangeTypeConfiguration>
</ChangeTypes>