<xmlconfig>

  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1999-05-28T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>


 <version ptype="INTEGER">1</version>

 <!-- First processor configuration -->
 <processor>
  <name>SetDefaults</name>
  <progid>MTPipeline.PropGenProcessor.1</progid>
  <description>
  This is a ruleset plug-in used to set the stock properties.
  </description>

  <!-- Processor specific configuration data -->
  <configdata>

    <default_actions>
      <action>
          <prop_name>namespace</prop_name>
          <prop_value>mt</prop_value>
      </action>

      <action>
          <prop_name>_Amount</prop_name>
          <prop_value ptype="decimal">10.99</prop_value>
      </action>

    </default_actions>

    
  </configdata>
 </processor>

  </mtconfigdata>
</xmlconfig>
