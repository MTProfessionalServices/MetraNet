	</attribute_value>

	<attribute_value pcentitytype="25">
		<property>Description</property>
		<attribute>overrideable</attribute>
		<value>true</value>
	</attribute_value>


  <!-- non-recurring charge -->

  <attribute_value pcentitytype="30">
    <property>Name</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>

  <attribute_value pcentitytype="30">
