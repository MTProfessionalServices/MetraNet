<mt_config>
	<!-- Starting with 6.0.5, the only supported attribute will be "overrideable".  PCWS will not recognize other attribute types  -->
	
  <!-- usage charge -->

  <attribute_value pcentitytype="10">
    <property>Name</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>

  <attribute_value pcentitytype="10">
    <property>DisplayName</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>

  <attribute_value pcentitytype="10">
    <property>DisplayNames</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>
  
  <attribute_value pcentitytype="10">
    <property>Description</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>


  <!-- aggregate charge -->

  <attribute_value pcentitytype="15">
    <property>Name</property>
    <attribute>overrideable</attribute>
    <value>true</value>
  </attribute_value>

  <attribute_value pcentitytype="15">
    <property>DisplayName</property>
