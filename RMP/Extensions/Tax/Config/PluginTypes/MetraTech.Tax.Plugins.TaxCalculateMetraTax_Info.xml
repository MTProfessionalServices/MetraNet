<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
    <ProgID>MetraTech.Tax.Plugins.TaxCalculateMetraTax</ProgID>
    <DisplayName>TaxCalculateMetraTax</DisplayName>
    <Description>Determines the amount of tax associated with the transaction using MetraTax.</Description>
    <Author>MetraTech Corp. (Engineering)</Author>
    <Category>Basic Usage</Category>
    <SupportsSessionSets>True</SupportsSessionSets>
    <TransactionalSupport>Supports</TransactionalSupport>
    <Editor>plgGenericPropertyGrid</Editor>
    <HelpURL></HelpURL>
    <IsTransactional>False</IsTransactional>
    <IsCodeGenerated>True</IsCodeGenerated>
    <DeprecatedVersion DeprecatedMessage=""></DeprecatedVersion>
    <AutoPipelineBindingProperties>
        <ptype>
            <dn>AccountID</dn>
            <type>int32</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>This account will be examined to determine country and country zone.  This will then be used with parameter tables containing MetraTax rates and other information to determine the tax rate.</description>
            <xpath>PipelineBinding/AccountID</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>Amount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The amount that should be taxed.</description>
            <xpath>PipelineBinding/Amount</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>InvoiceDate</dn>
            <type>timestamp</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>This is the date of the transaction.  The date is used to select the best fitting rate from the MetraTax parameter tables.</description>
            <xpath>PipelineBinding/InvoiceDate</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>IntervalID</dn>
            <type>int32</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The interval ID associated with the transaction.</description>
            <xpath>PipelineBinding/IntervalID</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>ProductCode</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>A code identifying the product.  This same code should be used in the MetraTax parameter table define the tax rate for the product. This field may be left blank and the value from GeneralConfig will be used.</description>
            <xpath>PipelineBinding/ProductCode</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>IsImpliedTax</dn>
            <type>boolean</type>
            <length></length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>If true, the amount already includes the tax. This field may be left blank and the value from GeneralConfig will be used.</description>
            <xpath>PipelineBinding/IsImpliedTax</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>RoundingAlgorithm</dn>
            <type>string</type>
            <length>-1</length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>The rounding algorithm to use.  Acceptable values are: NONE or BANK.  NONE means no rounding will be performed. BANK means banker's rounding will be performed. This field may be left blank and the value from GeneralConfig will be used.</description>
            <xpath>PipelineBinding/RoundingAlgorithm</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>RoundingDigits</dn>
            <type>int32</type>
            <length></length>
            <required>N</required>
            <defaultvalue></defaultvalue>
            <description>The number of rounding digits. 0 - no rounding, 1 round to 1 digit after decimal point, 2 for 2 digits, etc. This field may be left blank and the value from GeneralConfig will be used.</description>
            <xpath>PipelineBinding/RoundingDigits</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>FederalTaxAmount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The federal tax amount.</description>
            <xpath>PipelineBinding/FederalTaxAmount</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
    </ptype>
               <ptype>
            <dn>FederalTaxAmountRounded</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounded federal tax amount.</description>
            <xpath>PipelineBinding/FederalTaxAmountRounded</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>FederalTaxName</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The federal tax name. This comes from the configured MetraTax parameter table defining the rate.</description>
            <xpath>PipelineBinding/FederalTaxName</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>StateTaxAmount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The state tax amount.</description>
            <xpath>PipelineBinding/StateTaxAmount</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
    </ptype>
           <ptype>
            <dn>StateTaxAmountRounded</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounded state tax amount.</description>
            <xpath>PipelineBinding/StateTaxAmountRounded</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>StateTaxName</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The state tax name.</description>
            <xpath>PipelineBinding/StateTaxName</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>CountyTaxAmount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The county tax amount.</description>
            <xpath>PipelineBinding/CountyTaxAmount</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
    </ptype>
               <ptype>
            <dn>CountyTaxAmountRounded</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounded county tax amount.</description>
	    <xpath>PipelineBinding/CountyTaxAmountRounded</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>CountyTaxName</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The county tax name.</description>
            <xpath>PipelineBinding/CountyTaxName</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>LocalTaxAmount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The local tax amount.</description>
            <xpath>PipelineBinding/LocalTaxAmount</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
    </ptype>
               <ptype>
            <dn>LocalTaxAmountRounded</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounded local tax amount.</description>
	    <xpath>PipelineBinding/LocalTaxAmountRounded</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>LocalTaxName</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The local tax name.</description>
            <xpath>PipelineBinding/LocalTaxName</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>OtherTaxAmount</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>Other tax amount.</description>
            <xpath>PipelineBinding/OtherTaxAmount</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
    </ptype>
               <ptype>
            <dn>OtherTaxAmountRounded</dn>
            <type>decimal</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounded other tax amount.</description>
	    <xpath>PipelineBinding/OtherTaxAmountRounded</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>OtherTaxName</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>Other tax name.</description>
            <xpath>PipelineBinding/OtherTaxName</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>TaxChargeID</dn>
            <type>int64</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>An index that can be used to get detailed information about the tax if tax details were stored (based on configuration).  This value is used for the column id_tax_charge in t_tax_details.</description>
            <xpath>PipelineBinding/TaxChargeID</xpath>
            <Category></Category>
            <direction>Output</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
    </AutoPipelineBindingProperties>
    <AutoGeneralConfigProperties>
        <ptype>
            <dn>ShouldTaxDetailsBeStored</dn>
            <type>boolean</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>For each transaction tax, MetraNet can stored detalied information (in the t_tax_details table).  This value determines if these details are saved or not.</description>
            <xpath>GeneralConfig/ShouldTaxDetailsBeStored</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>DefaultIsImpliedTax</dn>
            <type>boolean</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>If true, the amount already includes the tax.</description>
            <xpath>GeneralConfig/DefaultIsImpliedTax</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>DefaultProductCode</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>A code identifying the product.  This same code should be used in the MetraTax parameter table define the tax rate for the product.</description>
            <xpath>GeneralConfig/DefaultProductCode</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>DefaultRoundingAlgorithm</dn>
            <type>string</type>
            <length>-1</length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The rounding algorithm to use.  Acceptable values are: NONE or BANK.  NONE means no rounding will be performed. BANK means banker's rounding will be performed.</description>
            <xpath>GeneralConfig/DefaultRoundingAlgorithm</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
        <ptype>
            <dn>DefaultRoundingDigits</dn>
            <type>int32</type>
            <length></length>
            <required>Y</required>
            <defaultvalue></defaultvalue>
            <description>The number of rounding digits. 0 - no rounding, 1 round to 1 digit after decimal point, 2 for 2 digits, etc.</description>
            <xpath>GeneralConfig/DefaultRoundingDigits</xpath>
            <Category></Category>
            <direction>Input</direction>
            <DefaultBinding></DefaultBinding>
        </ptype>
    </AutoGeneralConfigProperties>
</xmlconfig>
