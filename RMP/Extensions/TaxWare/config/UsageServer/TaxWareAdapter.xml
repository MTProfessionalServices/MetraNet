<?xml version="1.0" encoding="UTF-8"?>

<xmlconfig>
	<Vendor>TaxWare</Vendor>

	<!-- For sample invoices, this should be 'Y" -->
	<TaxDetailsNeeded ptype="BOOLEAN">TRUE</TaxDetailsNeeded>

	<!-- For sample invoices, this should be 'Y" -->
	<IsAuditingNeeded ptype="BOOLEAN">TRUE</IsAuditingNeeded>

	<!-- The maximum number of tax calculation errors permitted before stopping -->
	<MaximumNumberOfErrors ptype="INTEGER">0</MaximumNumberOfErrors>
	
	<HookQueryFolder>Queries\Tax\Customizable</HookQueryFolder>
	<!--<HookQueryFolder>Queries\Tax\Test</HookQueryFolder>-->
	<Queries>
	        <CreateTaxInputTable>__CREATE_TAXWARE_TAX_INPUT__</CreateTaxInputTable>
	        <CreateTaxInputIndexes>__CREATE_TAXWARE_TAX_INPUT_INDEXES__</CreateTaxInputIndexes>
	</Queries>
        <HookType>EOP</HookType>
</xmlconfig>
