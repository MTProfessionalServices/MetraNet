<xmlconfig>

  <mtsysconfigdata>
    <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
    <timeout ptype="INTEGER">30</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>

  <mtconfigdata>


 <version ptype="INTEGER">1</version>
 <!-- First processor configuration -->
 <processor>
  <name>CalendarCodeLookup</name>
  <progid>MetraPipeline.CalendarCodeLookup.1</progid>
  <description>
  	Sets calendar code in session based on "lookup_time_prop_name" property.
  	In PITest pipeline we will always look up calendar code based on the file located
  	in stage directory. However, real world implementation would probably use different calendar files
  	on account per account basis. A parameter table would need to be created to set "calendar_id_prop_name"
  	property in session. Current plugin checks for existence of calendar_id property in session
  </description>


  <inputs>
    <input type="DATETIME" internal_name="lookup_time_prop_name">Time</input>
    <input type="LONG" internal_name="offset_prop_name">timezoneoffset</input>
    <input type="LONG" internal_name="timezoneid_prop_name">timezoneID</input>
  </inputs>

  <outputs>
    <output type="STRING" internal_name="calendar_code_prop_name">CalendarCode</output>
  </outputs>
  

  <!-- Processor specific configuration data -->
  <configdata>
    <calendar_code_prop_name>CalendarCode</calendar_code_prop_name>
    <SetEnumCalendarCode ptype="BOOL">TRUE</SetEnumCalendarCode>
		<calendar_code_enum_space>metratech.com/calendar</calendar_code_enum_space>
		<calendar_code_enum_type>CalendarCode</calendar_code_enum_type>
  
    <lookup_time_prop_name>Time</lookup_time_prop_name>
    <offset_prop_name>timezoneoffset</offset_prop_name>
    <timezoneid_prop_name>timezoneID</timezoneid_prop_name>
    <calendar>calendar.xml</calendar> 
    <calendar_id_prop_name>calendar_id</calendar_id_prop_name> 
  </configdata>

 </processor>

  </mtconfigdata>

</xmlconfig>
