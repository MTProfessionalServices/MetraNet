<?xml version="1.0" encoding="UTF-8"?>

<xmlconfig>
  <mtsysconfigdata>
    <effective_date ptype="DATETIME">2001-04-06T21:46:29Z</effective_date>
    <timeout ptype="INTEGER">365</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>
  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <processor>
      <name>pcratelookup</name>
      <progid>MetraPipeline.PCRate.1</progid>
      <description></description>
      <inputs>
        <input type="LONG">Duration</input>
        <input type="DECIMAL">DecProp3</input>
      </inputs>
      <outputs>
        <output type="DECIMAL">Rate</output>
        <output type="LONG">MTI</output>
        <output type="DECIMAL">MinCharge</output>
        <output type="STRING">MinUOM</output>
        <output type="DECIMAL">SetupCharge</output>
        <output type="BOOLEAN">DecProp4Applic</output>
      </outputs>

      <configdata>

        <!-- lookup from a named parameter table -->
        <RateLookup>
          <ParamTable>metratech.com/rateconn</ParamTable>
        </RateLookup>

        <RateLookup>
          <ParamTable>metratech.com/decimalcalc</ParamTable>
        </RateLookup>

        <!-- lookup from a dynamically determined parameter table -->
<!--
        <RateLookup>
          <ParamTableID>PTID</ParamTableID>
        </RateLookup>
-->

        <properties>
	    <_Timestamp>_Timestamp</_Timestamp>
	    <_PriceableItemName>_PriceableItemName</_PriceableItemName>
	    <_AccountID>_AccountID</_AccountID>
	    <_AccountPriceList>_AccountPriceList</_AccountPriceList>
	    <_PriceableItemTypeID>_PriceableItemTypeID</_PriceableItemTypeID>
	    <_SubscriptionID>_SubscriptionID</_SubscriptionID>
	    <_UsageCycleID>_usageCycleID</_UsageCycleID>
	</properties>
      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
