<?xml version="1.0" encoding="utf-8"?>
<xmlconfig>
    <mtsysconfigdata>
        <effective_date ptype="DATETIME">1998-11-19T00:00:00Z</effective_date>
        <timeout ptype="INTEGER">30</timeout>
        <configfiletype>CONFIG_DATA</configfiletype>
    </mtsysconfigdata>
    <mtconfigdata>
        <version ptype="INTEGER">1</version>
        <!-- First processor configuration -->
        <processor>
            <name>timestampoverride</name>
            <progid>MetraPipeline.MTSQLInterpreter.1</progid>
            <description>set's the sessions timestamp value</description>
            <!--
  <autotest>
    <file>setstarttime.xml</file>
  </autotest>
  -->
            <inputs></inputs>
            <outputs></outputs>
            <!-- Processor specific configuration data -->
            <configdata>
                <Program><![CDATA[
	CREATE PROCEDURE settimestamp @Time	DATETIME @_Timestamp 	DATETIME OUTPUT
	AS
	SET @_Timestamp = @Time;
	 ]]></Program>
            </configdata>
        </processor>
    </mtconfigdata>
</xmlconfig>