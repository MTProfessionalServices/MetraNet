<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
                   xmlns:xsd="http://www.w3.org/2001/XMLSchema"
	                 xmlns="urn:nhibernate-mapping-2.2"
                   namespace="MetraTech.BusinessEntity.Common"
                   assembly="MetraTech.BusinessEntity">
	<class name="EnumEntry" table="t_enum_data" mutable="false">
    <meta attribute="category">CodeDriven</meta>
		<id
			name="Id"
			column="id_enum_data"
			type="System.Int32">
      <generator class="increment" />
		</id>
		<property
			name="DbEnumEntry"
      column="nm_enum_data"
			type="string"
			length="255"
			unique="true" />
	</class>
</hibernate-mapping>