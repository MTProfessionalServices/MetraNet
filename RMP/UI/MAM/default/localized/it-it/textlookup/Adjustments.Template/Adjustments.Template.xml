<?xml version="1.0" encoding="UTF-8"?>
<texts id="Adjustments.Template" type="collection">
  <description>
    Define the visible fields and their order for the price able item xxxxx/xxxxx
    for the Dialog Transaction Finder (Adjustment.Finder.asp)
  </description>
  <!--
      Id attribute syntax:
      TransactionFinder.Template:[ProductView-FQN](Index)
    -->
  <!-- metratech.com/audioconfcall -->
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(1)">
    <value>[IT]ConferenceID</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(2)">
    <value>[IT]ConferenceName</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(3)">
    <value>[IT]ConferenceSubject</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(4)">
    <value>[IT]OrganizationName</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(5)">
    <value>[IT]ServiceLevel</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/audioconfcall(6)">
    <value>[IT]ScheduledStartTime</value>
  </text>

  <!-- metratech.com/songdownloads -->
  <text id="TransactionFinder.Template:metratech.com/songdownloads(1)">
    <value>[IT]songs</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/songdownloads(2)">
    <value>mp3bytes</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/songdownloads(3)">
    <value>wavbytes</value>
  </text>
  <text id="TransactionFinder.Template:metratech.com/songdownloads(4)">
    <value>TotalSongs</value>
  </text>
</texts>