<xmlconfig>
  <version ptype="INTEGER">1</version>

  <!--
    Parameters that are used when Rebill is chosen to be done to Unknown Account
  -->
  <miu>
    <accountid>-1</accountid>
    <seid>-1</seid>
    <accountname>miurebill</accountname>
    <sename>miurebill</sename>
    <accountnamespace>miurebill</accountnamespace>
    <senamespace>miurebill</senamespace>
    <secorpname>miurebill</secorpname>
    <secorpnamespace>miurebill</secorpnamespace>
  </miu>
  <batchops>
    <!-- 
      Timeout for batch operations in seconds.
      Used during Rebill submit operations
    -->
    <batchtimeout>360</batchtimeout>
    <!-- Waits for 'batchretryinterval' seconds before retrying again -->
    <batchretryinterval>20</batchretryinterval>
    <!-- 
      Batch operation will fail if a batch is still 
      not completed after batchmaxretries retries 
     -->
    <batchmaxretries>5</batchmaxretries>
    
  </batchops>
    
</xmlconfig>
