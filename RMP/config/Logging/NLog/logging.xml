<xmlconfig>

 <!--  THIS FILE IS NOT USED TO CONFIGURE LOGGING   -->
 <!--                                               -->
 <!--  AMP uses the 3rd party software NLog for     -->
 <!--  logging.  The configuration files are        -->
 <!--  NLog.config for configuration of logging on  -->
 <!--  AMP master and NLog.slave.config for         -->
 <!--  slave logging.                               -->

 <version ptype="INTEGER">1</version>
 <logging_config>
  <logfilename>%temp%\mtLog.txt</logfilename>
  <loglevel ptype="INTEGER">2</loglevel>
  <logsocketlevel ptype="INTEGER">0</logsocketlevel>
  <logsocketport ptype="INTEGER">1468</logsocketport>
  <logsocketserver>localhost</logsocketserver> 
  <logsocketconnectiontype>tcp</logsocketconnectiontype>
  <logsocketfacility ptype="INTEGER">16</logsocketfacility>
  <logsockettag>MetraNet</logsockettag>
  <logrolloverage ptype="INTEGER">0</logrolloverage>
  <logfilterlevel ptype="INTEGER">0</logfilterlevel>
  <logfiltertag></logfiltertag>
  <logcircularbuffersize ptype="INTEGER">25</logcircularbuffersize>
 </logging_config>
</xmlconfig>
