<xmlconfig>

 <version ptype="INTEGER">1</version>
 <!-- Database configuration -->
 <logging_config>
  <logfilename>%s</logfilename>
  <loglevel ptype="INTEGER">5</loglevel>

  <!-- port number for remote monitoring of messages. 0=no echo to port -->
  <logsocket ptype="INTEGER">0</logsocket>

  <!-- days that will trigger a log file rollover. 0= no age rollover -->
  <logrolloverage ptype="INTEGER">0</logrolloverage>

  <!-- filter log file contents. 0= no filtering on loglevel-->
  <!-- if this log level is in a message, the circular buffer flushed to log file. -->
  <logfilterlevel ptype="INTEGER">0</logfilterlevel>

  <!-- filter log file contents. empty string= no filtering on application tag-->
  <!-- if this app tag is in a message, the circular buffer flushed to log file. -->
  <logfiltertag></logfiltertag>

 </logging_config>

</xmlconfig>