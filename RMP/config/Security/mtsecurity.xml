<!-- This file must be in RMP\config\security -->
<xmlconfig>
  <!-- Specify the specific implementation of ICryptoManager. Currently,
       either RSACryptoManager or MSCryptoManager
  -->
  <cryptoTypeName>MetraTech.Security.Crypto.MSCryptoManager</cryptoTypeName>
  <rsaConfig>
    <kmsIdentityGroup>MTDev</kmsIdentityGroup>
    <kmsClientConfigFile>security\key.cfg</kmsClientConfigFile>
    <kmsCertificatePwd encrypted="false">dsg20056</kmsCertificatePwd>
    <kmsKeyClass id="PaymentInstrument">CreditCardNumber</kmsKeyClass>
    <kmsKeyClass id="DatabasePassword">CreditCardNumber</kmsKeyClass>
    <kmsKeyClass id="ServiceDefProp">CreditCardNumber</kmsKeyClass>
    <kmsKeyClass id="QueryString">CreditCardNumber</kmsKeyClass>
    <kmsKeyClass id="PasswordHash">PasswordHash</kmsKeyClass>
	<kmsKeyClass id="PaymentMethodHash">PaymentInstHash</kmsKeyClass>
    <kmsKeyClass id="WorldPayPassword">CreditCardNumber</kmsKeyClass>
  </rsaConfig>
  
 </xmlconfig>
