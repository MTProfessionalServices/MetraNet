<!-- $Header$ -->

<!ELEMENT defaultstage ( #PCDATA ) >

<!ELEMENT direction ( ServiceID, stage ) >
<!ATTLIST direction _diffAttr NMTOKEN #IMPLIED Owner NMTOKEN #IMPLIED>

<!ELEMENT metername ( #PCDATA ) >

<!ELEMENT queue ( #PCDATA ) >

<!ELEMENT routeto ( queue ) >

<!ELEMENT ServiceID ( #PCDATA ) >
