<!-- Xml Schema for service definition file -->

<schema targetNamespace="http://www.metratech.com/MTNS"
        xmlns="http://www.w3.org/TR/1999/WD-xmlschema-1-19991217"
        xmlns:mt="http://www.metratech.com/MTNS">

<!-- 
	Sample V1.1 file;

	<defineservice>
		<msixdef>metratech.com\AccountCredit.msixdef</msixdef>
	</defineservice>

-->

	<!-- Top level element.  This must be first -->

	<element name="defineservice" type="DefineService"/>

	<type name="DefineService">
