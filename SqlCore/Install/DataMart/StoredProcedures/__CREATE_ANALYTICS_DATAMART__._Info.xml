<?xml version="1.0" encoding="utf-8"?>
<Query>
    <Action>StoredProcedure</Action>
    <Implementations>
        <Implementation>SqlServer</Implementation>
        <Implementation>Oracle</Implementation>
    </Implementations>
    <InstallTime>PostSync</InstallTime>
    <Schemas>
        <Schema>NetMeter</Schema>
    </Schemas>
    <Parameters>
        <Parameter>
            <Name>p_dt_now</Name>
            <DataType>timestamp</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The current time (UTC) or MetraTime</Description>
        </Parameter>
        <Parameter>
            <Name>p_id_run</Name>
            <DataType>int32</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The id_run of the adapter</Description>
        </Parameter>
        <Parameter>
            <Name>p_nm_currency</Name>
            <DataType>string</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The currency to use as the system currency</Description>
        </Parameter>
        <Parameter>
            <Name>p_nm_instance</Name>
            <DataType>string</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The identifier to use for this MetraNet instance</Description>
        </Parameter>
        <Parameter>
            <Name>p_n_months</Name>
            <DataType>int32</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The number of months back to calculate</Description>
        </Parameter>
        <Parameter>
            <Name>p_STAGINGDB_prefix</Name>
            <DataType>string</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <Description>The table prefix for staging tables</Description>
        </Parameter>
    </Parameters>
    <Description>Rebuilds the Analytics Datamart</Description>
    <ConversionAudit>
        <ConversionDuplicateName></ConversionDuplicateName>
        <CommonQuerySourceFile></CommonQuerySourceFile>
        <SqlServerQuerySourceFile></SqlServerQuerySourceFile>
        <OracleQuerySourceFile></OracleQuerySourceFile>
    </ConversionAudit>
</Query>