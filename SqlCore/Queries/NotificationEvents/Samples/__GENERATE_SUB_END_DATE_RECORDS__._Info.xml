<?xml version="1.0" encoding="utf-8"?>
<Query>
    <Action>Get</Action>
    <Implementations>
        <Implementation>SqlServer</Implementation>
        <Implementation>Oracle</Implementation>
    </Implementations>
    <Parameters>
        <Parameter>
            <Name>LAST_RUN_DATE</Name>
            <DataType>timestamp</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <SubstitutionSyntaxCheck></SubstitutionSyntaxCheck>
            <Description>Indicates the last time the adapter was run.</Description>
        </Parameter>
        <Parameter>
            <Name>CURRENT_RUN_DATE</Name>
            <DataType>timestamp</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <SubstitutionSyntaxCheck></SubstitutionSyntaxCheck>
            <Description>Indicates current run date.</Description>
        </Parameter>
        <Parameter>
            <Name>LANGUAGE_CODE</Name>
            <DataType>int32</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <SubstitutionSyntaxCheck></SubstitutionSyntaxCheck>
            <Description>Language code.</Description>
        </Parameter>
        <Parameter>
            <Name>NOTIFICATION_DAYS</Name>
            <DataType>int32</DataType>
            <ReplacementType>SqlParameter</ReplacementType>
            <SubstitutionSyntaxCheck></SubstitutionSyntaxCheck>
            <Description>Number of days before the subscription expires</Description>
        </Parameter>
    </Parameters>
    <Description>Sample query that gets the subscriptions ending within x days from last run date provided by the adapter.</Description>
    <ConversionAudit>
        <ConversionDuplicateName></ConversionDuplicateName>
        <CommonQuerySourceFile></CommonQuerySourceFile>
        <SqlServerQuerySourceFile></SqlServerQuerySourceFile>
        <OracleQuerySourceFile></OracleQuerySourceFile>
    </ConversionAudit>
</Query>