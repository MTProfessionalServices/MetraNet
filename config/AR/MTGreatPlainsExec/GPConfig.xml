<xmlconfig>
	<OutGoingIdentifier>/eConnect/RQeConnectOutType/eConnectProcessInfo/Outgoing</OutGoingIdentifier>
	<MoveBalancePrefix>MB</MoveBalancePrefix>    <!-- Max length of MoveBalancePrefix is 2 characters. -->
	<MoveBalanceSeperator>_</MoveBalanceSeperator><!-- Max length of MoveBalanceSeperator is one character. -->
  <OperatingCompanies>
    <Company>
  
      <Namespace>ar/external</Namespace>
      <ServerEntry>GreatPlainsServer</ServerEntry>
    </Company>
   
   <!-- <Company>
      <Namespace>ar/europe</Namespace>
      <ServerEntry>GreatPlainsServerEurope</ServerEntry>
    </Company> -->
  </OperatingCompanies>



  <!-- eConnect component to use for sending documents (default is ExecProcsXML.ExecStoredProcedure75 if not specified) -->
	<!-- Component for eConnect 7.5: ExecProcsXML.ExecStoredProcedure75 -->
	<!-- Component for eConnect 9.0: ExecProcsXML.ExecStoredProcedure9 -->
	<!-- Component for eConnect 10.0 (MetraTech implemented COM interface): MetraTech.AR.eConnectShim -->
	<eConnectObject>MetraTech.AR.eConnectShim</eConnectObject>

  <!-- eConnect connection string to use -->
  <!-- The values xxx are replaced with the correct entries in the servers.xml file for each operating company -->
  <!-- connection string for eConnect 7.5: Provider=SQLOLEDB.1;Persist Security Info=False;User ID=%%USERNAME%%;Password=%%PASSWORD%%;Initial Catalog=%%DATABASENAME%%;Data Source=%%SERVERNAME%% -->
  <!-- connection string for eConnect 8.0: Provider=SQLOLEDB.1;Integrated Security=SSPI;Persist Security Info=False;User ID=sa;Initial Catalog=%%DATABASENAME%%;Data Source=%%SERVERNAME%% -->
  <!-- connection string for eConnect 9.0: Provider=SQLOLEDB.1;Integrated Security=SSPI;Persist Security Info=False;User ID=sa;Initial Catalog=%%DATABASENAME%%;Data Source=%%SERVERNAME%%-->
  <!-- connection string for eConnect 10.0: Integrated Security=SSPI;Persist Security Info=False;User ID=sa;Initial Catalog=%%DATABASENAME%%;Data Source=%%SERVERNAME%% --> 	
  <eConnectConnectionString>Integrated Security=SSPI;Persist Security Info=False;User ID=sa;Initial Catalog=%%DATABASENAME%%;Data Source=%%SERVERNAME%%</eConnectConnectionString>
</xmlconfig>

  
