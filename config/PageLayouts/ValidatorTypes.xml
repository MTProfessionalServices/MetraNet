<?xml version="1.0" encoding="utf-8"?>
<!-- More Validators at:  http://extjs.com/forum/archive/index.php/t-4271.html -->
<!-- Add localized validator scripts to /MetraCare/JavaScript/Validators.js -->
<PageLayout>
<Validators>
  <!-- CORE EXT VALIDATORS-->
  <Validator>
    <Name>alpha</Name>
    <DisplayName>Alpha</DisplayName>
    <Description>Alphabetic string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>alphanum</Name>
    <DisplayName>Alpha Numeric</DisplayName>
    <Description>Alphanumeric string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
      <DataType>Int32</DataType>
      <DataType>Int64</DataType>
      <DataType>Double</DataType>
      <DataType>Decimal</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>email</Name>
    <DisplayName>E-Mail</DisplayName>
    <Description>Email string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>url</Name>
    <DisplayName>URL</DisplayName>
    <Description>URL string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <!-- CUSTOM VALIDATORS-->
  <Validator>
    <Name>password</Name>
    <DisplayName>Password</DisplayName>
    <Description>Password string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>phone</Name>
    <DisplayName>Phone</DisplayName>
    <Description>Phone string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>credit_card_number</Name>
    <DisplayName>Credit Card Number</DisplayName>
    <Description>Credit card number string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>   
  <Validator>
    <Name>cv_number</Name>
    <DisplayName>Credit Card Verification Number</DisplayName>
    <Description>Credit card verification number string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator>
  <Validator>
    <Name>middleinitial</Name>
    <DisplayName>Middle Initial validation</DisplayName>
    <Description>Middle initial string</Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
    </SupportedDataTypes>
  </Validator> 
   <Validator>
    <Name>allsymbolnum</Name>
    <DisplayName> All Alpha Numeric</DisplayName>
    <Description></Description>
    <SupportedDataTypes>
      <DataType>String</DataType>
      <DataType>Int32</DataType>
      <DataType>Int64</DataType>
      <DataType>Double</DataType>
      <DataType>Decimal</DataType>
    </SupportedDataTypes>
  </Validator>  
</Validators>
</PageLayout>