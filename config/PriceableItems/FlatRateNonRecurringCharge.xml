        <required_inputs>
            <input_val name="NRCAdjustmentAmount" type="DECIMAL" displayname="Amount to adjust non recurring charge" />
        </required_inputs>
        <outputs>
            <output_val name="TotalAdjustmentAmount" type="DECIMAL" displayname="Non Recurring Charge Total Adjustment Amount" />
        </outputs>
    </adjustment_type>
