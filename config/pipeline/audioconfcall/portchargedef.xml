<?xml version="1.0" encoding="UTF-8"?>

<xmlconfig>
  <mtsysconfigdata>
    <effective_date ptype="DATETIME">2001-04-06T21:46:29Z</effective_date>
    <timeout ptype="INTEGER">365</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>
  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <processor>
      <name>PortChargeDef</name>
      <progid>MetraPipeline.PCRate.1</progid>
      <description>Look up unused/overused port definition data from the product catalog.</description>
      <inputs>
        
      </inputs>
       
      <outputs>
      	<output type="INTEGER">UnusedPortChargeDef</output>
      	<output type="INTEGER">UnusedThresholdFree</output>
      	<output type="INTEGER">UnusedPortDelta</output>
      	<output type="INTEGER">UnusedAdvancedPercentage</output>
      	<output type="INTEGER">UnusedExcludeRoles</output>
	<output type="INTEGER">OverusedPortChargeDef</output>
      	<output type="INTEGER">OverusedThresholdFree</output>
      	<output type="INTEGER">OverusedPortDelta</output>
      	<output type="INTEGER">OverusedAdvancedPercentage</output>
      	<output type="INTEGER">OverusedExcludeRoles</output>
      </outputs>

      <configdata>

        <!-- lookup from a named parameter table -->
        <RateLookup>
          <ParamTable>metratech.com/unusedportdef</ParamTable>
        </RateLookup>
        <RateLookup>
          <ParamTable>metratech.com/overusedportdef</ParamTable>
        </RateLookup>
        <!-- lookup from a dynamically determined parameter table -->
<!--
        <RateLookup>
          <ParamTableID>PTID</ParamTableID>
        </RateLookup>
-->
        <!-- Initialize the parameter table cache when this
             plug-in is initially loaded.

             This should boost performance if you have a lot of
             rate schedules that are being accessed only a few
             times, such as with ICB (Individual Case Based) rates.
         -->
        <InitParamTableCache ptype="BOOLEAN">FALSE</InitParamTableCache>

      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
