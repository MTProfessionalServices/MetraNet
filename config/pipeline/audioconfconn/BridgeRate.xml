<?xml version="1.0" encoding="UTF-8"?>

<xmlconfig>
  <mtsysconfigdata>
    <effective_date ptype="DATETIME">2001-04-06T21:46:29Z</effective_date>
    <timeout ptype="INTEGER">365</timeout>
    <configfiletype>CONFIG_DATA</configfiletype>
  </mtsysconfigdata>
  <mtconfigdata>
    <version ptype="INTEGER">1</version>
    <processor>
      <name>BridgeRate</name>
      <progid>MetraPipeline.PCRate.1</progid>
      <description>Look up the bridge rates for this connection from the product catalog.</description>

      <configdata>

        <!-- lookup from a named parameter table -->
        <RateLookup>
          <ParamTable>metratech.com/bridgerate</ParamTable>
        </RateLookup>

        <!-- lookup from a dynamically determined parameter table -->
<!--
        <RateLookup>
          <ParamTableID>PTID</ParamTableID>
        </RateLookup>
-->
        <!-- Initialize the parameter table cache when this
             plug-in is initially loaded.

             This should boost performance if you have a lot of
             rate schedules that are being accessed only a few
             times, such as with ICB (Individual Case Based) rates.
         -->
<InitParamTableCache ptype="BOOLEAN">TRUE</InitParamTableCache>

      </configdata>
    </processor>
  </mtconfigdata>
</xmlconfig>
