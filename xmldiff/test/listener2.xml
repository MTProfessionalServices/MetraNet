<?xml version="1.0"?>
<!DOCTYPE xmlconfig SYSTEM "http://localhost/validation/listener_1_0.dtd">

<xmlconfig xmlns:xsi="http://www.w3.org/1999/XMLSchema/instance" 
xsi:schemaLocation="http://localhost/validation/listener_1_0.xsd">

 <version ptype="INTEGER">1</version>
 <!-- listener configuration -->

 <metername>Meter1</metername>

 <routeto>
   <queue>RoutingQueue</queue>
 </routeto>

 <defaultstage>Audioconfcall</defaultstage>

 <!-- Payment Server starts here -->
 <direction>
   <ServiceID ptype="ID">metratech.com/quotepreauth</ServiceID>
   <stage>quote</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/postauth</ServiceID>
   <stage>postauth</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/settle</ServiceID>
   <stage>settle</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/credit</ServiceID>
   <stage>credit</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/debit</ServiceID>
   <stage>debit</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/validatecard</ServiceID>
   <stage>validatecard</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/findbynumberdatetype</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/findupdatebynumberdatetype</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/addaccount</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/updateaccount</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/deleteaccount</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/findbyaccountid</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/findbyaccountidlast4type</ServiceID>
   <stage>acctccmap</stage>
 </direction>

 <direction>
   <ServiceID ptype="ID">metratech.com/validatecardwithoutaccount</ServiceID>
   <stage>validatecardwithoutaccount</stage>
 </direction>



</xmlconfig>
