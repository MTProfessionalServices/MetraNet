<?xml version="1.0"?>
<!DOCTYPE xmlconfig SYSTEM "http://localhost/validation/viewtoaspmapping.dtd">

<xmlconfig xsi:schemaLocation="http://localhost/validation/viewtoaspmapping.xsd">
